{
  "manifest_version": 3,
  "name": "Simplest Tabs",
  "version": "1.1",
  "description": "Streamline Your Tab Management. Save, search, and navigate with ease using Simplest Tabs.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  }
}
